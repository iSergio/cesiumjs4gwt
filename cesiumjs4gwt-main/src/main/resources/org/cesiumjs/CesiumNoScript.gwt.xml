<!-- If you inherits this script, you need add Cesium.js and css files into you HTML Header-->
<!-- When updating your version of GWT, you should also update this DTD reference,
	so that your app can take advantage of the latest GWT module capabilities. -->
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.2//EN"
        "http://www.gwtproject.org/doctype/2.8.2/gwt-module.dtd">
<module>
    <inherits name="com.google.gwt.user.User" />
    <inherits name="com.google.gwt.json.JSON" />

    <entry-point class="org.cesiumjs.cs.CesiumEntryPoint" />

    <!--<define-linker name="cesium" class="org.cesiumjs.linker.CesiumLinker" />-->
    <!--<add-linker    name="cesium"/>-->

    <define-linker name="cesiumScriptInjector" class="org.cesiumjs.linker.CesiumScriptInjector" />
    <add-linker    name="cesiumScriptInjector"/>

    <set-configuration-property name='xsiframe.failIfScriptTag' value='FALSE'/>

    <source path="cs"/>
</module>